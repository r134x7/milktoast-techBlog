{{#each posts as |post| }}
<div class="box">
  <div class="head">
    <h4>
      <a href="/post/{{post.id}}">{{post.title}}</a>
    </h2>
    <p>Posted by {{post.user.name}} on {{format_date post.date_created}}</p>
  </div>
  <div class="content">
      <p>
      {{post.content}}
    </p>
  </div>
</div>
{{/each}}
